{
  "name": "fanzone-smart-contractss",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf typechain && npx hardhat clean && npx hardhat compile",
    "test": "npx hardhat compile && npx hardhat test",
    "coverage": "npm run build && npx hardhat coverage --temp artifacts --network coverage",
    "clean:setup-ecosystem-results": "find setup-ecosystem-data -name \"*result*\" -type d -exec rm -rf {} +",
    "fmt": "npx prettier --write 'contracts/**/*.sol' './{setup-ecoystem-data,tasks,test,utils}/**/*.ts'",
    "lint:sol": "npx solhint 'contracts/**/*.sol'",
    "prepare": "husky install"
  },
  "author": "fanzone-media",
  "license": "MIT",
  "dependencies": {
    "@lukso/universalprofile-smart-contracts": "git+ssh://git@github.com/fanzone-media/lsp-universalprofile-smart-contracts#54b9c6e13772435fe594eb322b6103dd2e881f1b",
    "@openzeppelin/contracts": "^4.3.0",
    "@openzeppelin/contracts-upgradeable": "^4.3.0",
    "@pinata/sdk": "^1.1.23",
    "axios": "^0.24.0",
    "chalk": "^4.1.1",
    "csv-parse": "^4.16.0",
    "csv-stringify": "^5.6.2",
    "erc725.js": "0.5.5",
    "ethers": "^5.1.4",
    "image-size": "^1.0.0",
    "snake-case": "^3.0.4",
    "solidity-bytes-utils": "^0.8.0",
    "strip-ansi": "^5.2.0"
  },
  "lint-staged": {
    "*.js": "prettier --write",
    "*.ts": "prettier --write",
    "*.json": "prettier --write",
    "*.sol": [
      "prettier --write",
      "solhint"
    ]
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.0",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^16.11.7",
    "chai": "^4.3.4",
    "dotenv": "^9.0.2",
    "ethereum-waffle": "^3.2.2",
    "hardhat": "^2.2.1",
    "hardhat-contract-sizer": "^2.3.0",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.1",
    "prettier-plugin-solidity": "^1.0.0-beta.15",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^5.0.0",
    "typescript": "^4.2.4"
  }
}
